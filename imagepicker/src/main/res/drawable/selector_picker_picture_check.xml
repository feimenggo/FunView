<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="@color/transparent" android:state_enabled="false">
        <shape />
    </item>
    <item android:color="@color/white" android:state_enabled="true" android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="100dp" />
            <stroke android:width="1dp" android:color="@color/white" />
            <solid android:color="@color/checked" />
        </shape>
    </item>
    <item android:color="@color/transparent" android:state_enabled="true" android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="100dp" />
            <stroke android:width="1dp" android:color="@color/white" />
            <solid android:color="#33020202" />
        </shape>
    </item>
</selector>